   <PasswordField fx:id="senha" prefHeight="25.0" prefWidth="134.0" promptText="Senha" GridPane.columnSpan="3" GridPane.rowIndex="3" />
   <PasswordField fx:id="confirmarSenha" prefHeight="25.0" prefWidth="270.0" promptText="Confirmar senha" GridPane.columnSpan="3" GridPane.rowIndex="4" />
   <Button fx:id="botaoCadastrar" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#cadastrar" prefHeight="44.0" prefWidth="481.0" text="Cadastrar" GridPane.columnSpan="3" GridPane.rowIndex="5">
